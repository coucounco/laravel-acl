{
    "name": "rohsyl/laravel-acl",
    "description": "Access Control List for Laravel",
    "authors": [
        {
            "name": "Sylvain Roh",
            "email": "syzin12@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.3",
        "illuminate/auth": "^6.0|^7.0",
        "illuminate/container": "^6.0|^7.0",
        "illuminate/contracts": "^6.0|^7.0",
        "illuminate/database": "^6.0|^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "rohsyl\\LaravelAcl\\": "src"
        },
        "files": [
            "src/Helpers/acl.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "rohsyl\\LaravelAcl\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "rohsyl\\LaravelAcl\\ServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "./vendor/phpunit/phpunit/phpunit"
    }
}
