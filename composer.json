{
    "name": "coucounco/laravel-acl",
    "description": "Access Control List for Laravel",
    "authors": [
        {
            "name": "Sylvain Roh",
            "email": "syzin12@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.0.2",
        "illuminate/auth": "^9.0",
        "illuminate/container": "^9.0",
        "illuminate/contracts": "^9.0",
        "illuminate/database": "^9.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0",
        "phpunit/phpunit": "^9"
    },
    "autoload": {
        "psr-4": {
            "rohsyl\\LaravelAcl\\": "src"
        },
        "files": [
            "src/Helpers/acl.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "rohsyl\\LaravelAcl\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "rohsyl\\LaravelAcl\\ServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "./vendor/phpunit/phpunit/phpunit"
    }
}
